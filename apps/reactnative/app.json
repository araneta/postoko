{
  "expo": {
    "name": "Your POS App",
    "slug": "your-pos-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.boltexpo.pos",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes and take product photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to save and upload product images.",
        "NSBluetoothAlwaysUsageDescription": "This app uses Bluetooth to connect to receipt printers.",
        "NSBluetoothPeripheralUsageDescription": "This app uses Bluetooth to connect to receipt printers."
      }
    },
    "android": {
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 23,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.posapp",
      "buildType": "apk",
      "permissions": [
        "CAMERA",
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7ae91d61-ae40-44ed-9473-0ffd65421fc9"
      }
    },
    "owner": "aldopraherda"
  }
}
